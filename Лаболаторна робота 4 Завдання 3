x1=float(input("x1="))
y1=float(input("y1="))
x2=float(input("x2="))
y2=float(input("y2="))
x3=float(input("x3="))
y3=float(input("y3="))
x4=float(input("x4="))
y4=float(input("y4="))
a=((x2-x1)**2)+((y2-y1)**2)**0.5
b=((x3-x2)**2)+((y3-y2)**2)**0.5
c=((x4-x3)**2)+((y4-y3)**2)**0.5
d=((x1-x4)**2)+((y1-y4)**2)**0.5
if a==b and b==c and c==d:
    print("Точки є вершинами ромба")
else:
    print("Точки не є вершинами ромба")
